{
  "name": "gym-management-nest-js",
  "version": "0.0.1",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "rebuild": "npm run build && npm run start",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "typeorm": "node --require ts-node/register ./node_modules/typeorm/cli.js",
    "typeorm:setup": "(rm ormconfig.json || :) && ts-node -r tsconfig-paths/register src/scripts/create-ormconfig.script.ts",
    "migration:generate": "npm run build && npx typeorm migration:generate -d src/migrations -n",
    "migration:run": "npm run build && npx typeorm migration:run",
    "migration:revert": "npx typeorm migration:revert"
  },
  "dependencies": {
    "@hapi/joi": "^17.1.1",
    "@nestjs-modules/mailer": "^1.5.1",
    "@nestjs/bull": "^0.3.1",
    "@nestjs/common": "^7.6.5",
    "@nestjs/config": "^0.5.0",
    "@nestjs/core": "^7.0.0",
    "@nestjs/jwt": "^7.2.0",
    "@nestjs/passport": "^7.1.5",
    "@nestjs/platform-express": "^7.0.0",
    "@nestjs/schedule": "^0.4.1",
    "@nestjs/typeorm": "^7.1.4",
    "aws-sdk": "^2.792.0",
    "bcryptjs": "^2.4.3",
    "bull": "^3.20.0",
    "cache-manager": "^3.4.0",
    "cache-manager-redis-store": "^2.0.0",
    "class-transformer": "^0.3.1",
    "class-validator": "^0.12.2",
    "config": "^3.3.2",
    "dotenv": "^8.2.0",
    "handlebars": "^4.7.6",
    "helmet": "^4.1.0",
    "lodash": "^4.17.20",
    "moment": "^2.29.1",
    "ms": "^2.1.3",
    "nestjs-rate-limiter": "^2.7.0",
    "nodemailer": "^6.4.17",
    "passport": "^0.4.1",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.0",
    "pg": "^8.4.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^6.5.4",
    "sharp": "^0.26.2",
    "ts-node": "9.0.0",
    "typeorm": "^0.2.29",
    "typescript": "^3.7.4",
    "uuid": "^8.3.1"
  },
  "devDependencies": {
    "@nestjs/cli": "^7.0.0",
    "@nestjs/schematics": "^7.0.0",
    "@nestjs/testing": "^7.0.0",
    "@types/aws-sdk": "^2.7.0",
    "@types/bcryptjs": "^2.4.2",
    "@types/bull": "^3.14.4",
    "@types/cache-manager": "^2.10.3",
    "@types/express": "^4.17.3",
    "@types/hapi__joi": "^17.1.6",
    "@types/jest": "26.0.10",
    "@types/lodash": "^4.14.165",
    "@types/moment": "^2.13.0",
    "@types/multer": "^1.4.4",
    "@types/node": "^13.9.1",
    "@types/passport-google-oauth20": "^2.0.4",
    "@types/passport-jwt": "^3.0.3",
    "@types/pg": "^7.14.9",
    "@types/sharp": "^0.26.1",
    "@types/supertest": "^2.0.8",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "3.9.1",
    "@typescript-eslint/parser": "3.9.1",
    "eslint": "7.7.0",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-import": "^2.20.1",
    "jest": "26.4.2",
    "prettier": "^1.19.1",
    "supertest": "^4.0.2",
    "ts-jest": "26.2.0",
    "ts-loader": "^6.2.1",
    "tsconfig-paths": "^3.9.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
